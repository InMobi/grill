<configuration>
  <property>
    <name>grill.server.user.resolver.type</name>
    <value>DATABASE</value>
  </property>
  <property>
    <name>grill.server.user.resolver.db.driver.name</name>
    <value>org.hsqldb.jdbcDriver</value>
  </property>
  <property>
    <name>grill.server.user.resolver.db.jdbc.url</name>
    <value>jdbc:hsqldb:target/userconfig_hsql.db</value>
  </property>
  <property>
    <name>grill.server.user.resolver.db.jdbc.username</name>
    <value>SA</value>
  </property>
  <property>
    <name>grill.server.user.resolver.db.jdbc.password</name>
    <value></value>
  </property>
  <property>
    <name>grill.server.user.resolver.db.keys</name>
    <value>grill.session.cluster.user,mapred.job.queue.name</value>
  </property>
  <property>
    <name>grill.server.user.resolver.db.query</name>
    <value>select user_clusteruser.clusteruser, department_queue.queue from user_clusteruser, user_department, department_queue where user_clusteruser.username=user_department.username and user_department.department=department_queue.department and user_clusteruser.username=?</value>
  </property>
</configuration>